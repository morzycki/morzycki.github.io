@Article{morzycki:goddamn_mp,
    author = {Marcin Morzycki},
    year = {2011},
    title = {Expressive Modification and the Licensing of Measure Phrases},
    journal = {Journal of Semantics},
    volume = {28},
    number = {3},
    pages = {401--411},
    abstract = {The aim of this squib is to call attention to the incompatibility of measure phrases and expressive modifiers such as \emph{goddamn}, and to demonstrate that this incompatibility can be explained straightforwardly by supplementing a somewhat modified version of the approach to measure-phrase interpretation proposed in Schwarzschild (2005) with a few minimal assumptions about what expressive modifiers mean.}
}
